@{
    Layout = null;

}


<div class="card">
    <div class="card-header h3">@ViewBag.Title</div>
    <div class="card-body">@ViewBag.Body</div>
    <div class="footer">
        <div class="d-flex justify-content-between">
            &nbsp;                
                <button type="button" id="btnYa" class="btn btn-success float-end" asp-action="profile" asp-controller="Doctor"><i class="fas fa-save"></i>Yes</button>
        </div>
    </div>
</div>

<script>
    $("#btnYa").click(function(){
        location.reload();
    })
</script>